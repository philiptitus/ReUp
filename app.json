{
  "name": "KiliEcoSpot",
  "displayName": "KiliEcoSpot",
  "owner": "philiptitus",
  "expo": {
    "name": "KiliEcoSpot",
    "slug": "re-Up",
    "scheme": "reup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo11.png",
    "splash": {
      "image": "./assets/images/logo11.png",
      "resizeMode": "contain",
      "backgroundColor": "#b4b3f4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "icon": "./assets/images/logo12.png",
      "package": "com.reup.byphil",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo13.png",
        "backgroundImage": "./assets/images/app-icon-android-adaptive-background.png"
      },
      "splash": {
        "image": "./assets/images/logo11.png",
        "resizeMode": "contain",
        "backgroundColor": "#b4b3f4"
      },
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "FOREGROUND_SERVICE"
      ]
    },
    "ios": {
      "icon": "./assets/images/app-icon-ios.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.reup.byphil",
      "splash": {
        "image": "./assets/images/logo11.png",
        "tabletImage": "./assets/images/splash-logo-ios-tablet.png",
        "resizeMode": "contain",
        "backgroundColor": "#b4b3f4"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone",
        "NSLocationWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to access your location",
        "NSLocationAlwaysUsageDescription": "Allow $(PRODUCT_NAME) to always access your location"
      }
    },
    "web": {
      "favicon": "./assets/images/app-icon-web-favicon.png",
      "splash": {
        "image": "./assets/images/splash-logo-web.png",
        "resizeMode": "contain",
        "backgroundColor": "#191015"
      },
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ],
      "expo-font",
      "expo-camera"
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  },
  "ignite": {
    "version": "UNKNOWN"
  }
}
